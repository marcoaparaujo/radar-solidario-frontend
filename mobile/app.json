{
  "name": "Radar Solidário",
  "displayName": "RadarSolidario",
  "expo": {
    "version": "0.0.1",
    "privacy": "public",
    "slug": "radar-solidario",
    "name": "Radar Solidário",
    "orientation": "portrait",
    "entryPoint": "./src/index.js",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./src/assets/images/icon.png",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./src/assets/images/splash.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.solidario.radar"
    }
  }
}
